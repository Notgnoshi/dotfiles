[user]
	name = Austin Gill
	email = notgnoshi@gmail.com
[core]
	editor = vim 
	fileMode = false
	eol = lf
	autocrlf = input
[credential]
	helper = cache
	help = cache --timeout=60000
[push]
	default = simple
[alias]
	lg    = log --color --graph --pretty=format:'%C(red)%h%C(reset) -%C(yellow)%d%C(reset) %s %C(green)(%cr) %C(bold blue)<%an>%C(reset)' --abbrev-commit --decorate
	ll    = log --pretty=format:'%C(red)%h%C(reset) -%C(yellow)%d%C(reset) %s %C(bold blue) <%an>' --decorate --numstat --abbrev-commit
	co    = checkout
	amend =  commit --amend --no-edit
	st    = status
	b     = branch

